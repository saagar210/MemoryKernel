{
  "service_contract_source_version": "service-contract-source.v1",
  "expected_service_contract_version": "service.v3",
  "expected_api_contract_version": "api.v1",
  "integration_baseline": "integration/v1",
  "liveness_endpoint": "/v1/health",
  "readiness_endpoint": "/v1/ready",
  "error_code_enum": [
    "invalid_json",
    "validation_error",
    "context_package_not_found",
    "write_conflict",
    "write_failed",
    "schema_unavailable",
    "migration_failed",
    "query_failed",
    "context_lookup_failed",
    "internal_error"
  ],
  "non_2xx_envelope_policy": {
    "requires": [
      "service_contract_version",
      "error.code",
      "error.message"
    ],
    "optional": [
      "error.details"
    ],
    "forbids": [
      "legacy_error",
      "api_contract_version"
    ]
  }
}
